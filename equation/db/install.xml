<?xml version="1.0" encoding="UTF-8" ?>

<XMLDB PATH="blocks/equation/db" VERSION="20181030" COMMENT="XMLDB file for Moodle blocks/equation"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">

    <TABLES>

        <TABLE NAME="block_equation" COMMENT="equations">

            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" PRECISION="2" NOTNULL="false" SEQUENCE="true"/>
                <FIELD NAME="a" TYPE="float" LENGTH="10" PRECISION="2" NOTNULL="false" SEQUENCE="false" COMMENT="a"/>
                <FIELD NAME="b" TYPE="float" LENGTH="10" PRECISION="2" NOTNULL="false" SEQUENCE="false" COMMENT="b"/>
                <FIELD NAME="c" TYPE="float" LENGTH="10" PRECISION="2" NOTNULL="false" SEQUENCE="false" COMMENT="c"/>
                <FIELD NAME="x1" TYPE="text" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="x1"/>
                <FIELD NAME="x2" TYPE="text" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="x2"/>
            </FIELDS>

            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>

        </TABLE>
    </TABLES>


</XMLDB>
